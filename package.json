  "author": "Shkaper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/webdriverio/wdio-spec-reporter/issues"
  },
  "homepage": "https://github.com/webdriverio/wdio-spec-reporter#readme",
  "dependencies": {
    "babel-runtime": "^5.8.25",
    "colors": "1.1.2",
