    "codeclimate-test-reporter": "^0.1.1",
    "eslint": "^1.4.1",
    "eslint-config-standard": "^4.3.2",
    "eslint-plugin-standard": "^1.3.0",
    "mocha": "^2.3.3",
    "should": "^7.1.0",
    "sinon": "^1.17.1"
